{
  "url": "https://api.linkedapi.io/automation/stats/actions?start=2024-01-01T00:00:00Z&end=2024-01-01T00:01:00Z",
  "method": "GET",
  "headers": {
    "linked-api-token": "{{parameters.linkedApiToken}}",
    "identification-token": "{{parameters.identificationToken}}",
    "client": "make",
    "Content-Type": "application/json"
  },
  "response": {
    "metadata": {
      "type": "text",
      "value": "Linked API"
    },
    "valid": "{{!body.criticalError}}",
    "error": {
      "message": "{{body.criticalError.type}}: {{body.criticalError.message}}"
    }
  },
  "log": {
    "sanitize": [
      "request.headers.linked-api-token",
      "request.headers.identification-token"
    ]
  }
}
