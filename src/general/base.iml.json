{
  "baseUrl": "https://api.linkedapi.io/automation",
  "headers": {
    "linked-api-token": "{{connection.linkedApiToken}}",
    "identification-token": "{{connection.identificationToken}}",
    "client": "make",
    "Content-Type": "application/json"
  },
  "response": {
    "valid": "{{!body.criticalError}}",
    "error": {
      "message": "{{body.criticalError.type}}: {{body.criticalError.message}}"
    }
  },
  "log": {
    "sanitize": [
      "request.headers.linked-api-token",
      "request.headers.identification-token"
    ]
  }
}
